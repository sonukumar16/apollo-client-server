{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": true,
    "ecmaVersion": 2017 
  },
  "rules": {
    "strict": 0,
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2
  },
  "extends": [
    "standard",
    "prettier",
    "prettier/standard"
  ],
  "plugins": [
    "import",
    "prettier",
    "standard"
  ],
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  }
}
